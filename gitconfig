[user]
  name = W. Evan Sheehan
	email = evan.sheehan@noaa.gov
[alias]
  st = status -s -b
  ci = commit --verbose
  co = checkout
  graph = log --graph --decorate --oneline
  sup = submodule update --remote
	l = log --oneline
	lp = !git branch --merged | grep -v \"^*\" | xargs git branch -d
[push]
  default = simple
	autoSetupRemote = true
[core]
  editor = $EDITOR
[diff]
  submodule = log
[config]
  rebase = false
[init]
	defaultBranch = main
[filter "lfs"]
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
